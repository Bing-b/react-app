{
  "files": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.node.json"
    }
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "#/*": ["types/*"]
    },
    "module": "ESNext",
    "moduleResolution": "node",
    "target": "ES2020",
    "types": ["node"]
  },
  "include": ["src", "types"],
  "exclude": ["node_modules", "dist"]
}
